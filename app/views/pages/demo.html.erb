<div class="whatever_content">

  <h2>This is sample content generated by random, preset interests for this demo.</h2>

  <% if @medium_content.present? %>
    <h3> via <a href="http://www.medium.com" target="_blank">Medium</a></h3>
    <div class="content_container">
      <%== @medium_content %>
    </div>
  <% end %>

  <%== @soundcloud_content if @soundcloud_content %>

  <%if @youtube_content.present? %>
    <div class="video_container">
      <%== @youtube_content if @youtube_content %>
    </div>
  <%end%>

  <div class="demo-link-a">
    <a class="demo-home demo" data-role="button" href="/">Back to Home</a>
  </div>

  <div class="demo-link-b">
    <a class="demo-hopp demo" data-role="button" href="javascript:history.go(0)">Hopp to new Content</a>
  </div>

  <div class="demo-sign-up">
    <a href="/users/new" class="demo" data-role="button">Sign Up</a>
    <p class="small">Signing up lets view content personalized for you based on your available time and interests.</p>
</div>